<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.szc.enterprise.web.dao.dao.UserMapper" >
  <resultMap id="user" type="com.szc.enterprise.web.model.User" >
    <result column="UserID" property="probeid" jdbcType="INTEGER" />
    <result column="UserName" property="username" jdbcType="VARCHAR" />
    <result column="Password" property="password" jdbcType="VARCHAR" />
  </resultMap>
  <select id="getUserByUserNameandPassword" resultMap="user" parameterType="map">
  SELECT * FROM user WHERE UserName = #{username} AND Password = #{password}
  </select>
  
</mapper>